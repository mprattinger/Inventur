<Window x:Class="Inventur.App.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Inventur.App"
        xmlns:dataEntry="clr-namespace:Inventur.App.Modules.DataEntryModule.Views"
        xmlns:dataList="clr-namespace:Inventur.App.Modules.DataListModule.Views"
        xmlns:export="clr-namespace:Inventur.App.Modules.ExportModule.Views"
        mc:Ignorable="d" 
        Width="768"
        Title="Lautner Inventur Tool 2.0" Height="600">
    <Grid>
        <StackPanel>
            <export:ExportView Margin="386 10 200 0"/>
            <dataEntry:DataEntryView />
            <dataList:DataListView Margin="20 20 20 0" />
        </StackPanel>
    </Grid>
</Window>
